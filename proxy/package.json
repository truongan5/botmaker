    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.39.2",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
